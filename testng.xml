<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression testing" parallel="tests" thread-count="2">

    <test name="Admin Role Test">
        <parameter name="browserName" value="chrome"></parameter>
        <parameter name="useGrid" value="false"></parameter>
        <classes>
            <class name="Admin_Role_Chrome"/>
        </classes>
    </test>

    <test name="Create new user">
        <parameter name="browserName" value="chrome"></parameter>
        <parameter name="useGrid" value="false"></parameter>
        <classes>
            <class name="CreateNewUser"/>
        </classes>
    </test>

</suite>

